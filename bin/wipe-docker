#!/bin/bash

(
    for pattern in \
    "datawire/telepresence-*-OSX-*" \
    "datawire/telepresence-*-LNX-*" \
    "ark3/telepresence-*" \
    "host.docker.internal:31000/*" \
    "localhost:31000/*" \
    "quay.io/datawire/*-dev"
    do
        docker images -q "$pattern"
    done
) | sort -u | xargs docker rmi -f
docker system prune --volumes -f > /dev/null
