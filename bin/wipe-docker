#!/bin/bash

docker images "datawire/telepresence-*-OSX-*" --format '{{.Repository}}:{{.Tag}}' | xargs docker rmi
docker images "datawire/telepresence-*-LNX-*" --format '{{.Repository}}:{{.Tag}}' | xargs docker rmi
docker images "ark3/telepresence-*" --format '{{.Repository}}:{{.Tag}}' | xargs docker rmi
docker system prune --volumes -f > /dev/null
