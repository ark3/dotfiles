#!/usr/bin/env python

"""
Print the absolute path of every pathname specified on the command line.
"""

import os
import sys


def main(args):
    for arg in args:
        res = os.path.abspath(arg)
        if not os.path.exists(res):
            print "X",

        print res

if __name__ == "__main__":
    main(sys.argv[1:])
